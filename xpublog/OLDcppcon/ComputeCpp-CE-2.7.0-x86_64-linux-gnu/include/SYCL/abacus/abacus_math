/// @return     A abacus_float16.

inline abacus_float16 ABACUS_API remquo(abacus_float16 x, abacus_float16 y, abacus_int16 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

