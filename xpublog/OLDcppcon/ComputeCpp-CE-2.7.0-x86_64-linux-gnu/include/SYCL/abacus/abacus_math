#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_nextafter.
/// @param[in] x A abacus_float.
/// @param[in] y A abacus_float.
/// @return      A abacus_float.
inline abacus_float ABACUS_API nextafter(abacus_float x, abacus_float y) {
  return __abacus_nextafter(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

/// @brief x^y.
/// @param[in] x A abacus_float.
/// @param[in] y A abacus_float.
/// @return      A abacus_float.
