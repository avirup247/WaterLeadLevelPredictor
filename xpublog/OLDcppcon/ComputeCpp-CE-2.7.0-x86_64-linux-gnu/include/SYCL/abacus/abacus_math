/// @return     A abacus_double2.

inline abacus_double2 ABACUS_API remquo(abacus_double2 x, abacus_double2 y, abacus_int2 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

