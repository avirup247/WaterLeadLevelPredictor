#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_double3.
/// @param[in]  y        A abacus_double3.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_double3.

inline abacus_double3 ABACUS_API remquo(abacus_double3 x, abacus_double3 y, abacus_int3 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

