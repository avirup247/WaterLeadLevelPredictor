/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float4, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float4 ABACUS_EXPORT_API __abacus_remquo(abacus_float4 x, abacus_float4 y, abacus_int4 * quotient);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float4.

inline abacus_float4 ABACUS_API remquo(abacus_float4 x, abacus_float4 y, abacus_int4 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float4, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float4 ABACUS_EXPORT_API __abacus_remquo(abacus_float4 x, abacus_float4 y, __global abacus_int4 * quotient);

/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float4.

inline abacus_float4 ABACUS_API remquo(abacus_float4 x, abacus_float4 y, __global abacus_int4 * quotient) {
  return __abacus_remquo(x, y, quotient);
}

/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float4, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float4 ABACUS_EXPORT_API __abacus_remquo(abacus_float4 x, abacus_float4 y, __local abacus_int4 * quotient);

/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float4.
/// @param[in]  y        A abacus_float4.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float4.

inline abacus_float4 ABACUS_API remquo(abacus_float4 x, abacus_float4 y, __local abacus_int4 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#endif

/// @brief Round to nearest even integer.
/// @param[in] x A abacus_float4.
/// @return      A abacus_float4.
///
/// Standards compliant implementation of OpenCL 1.2 rint.
