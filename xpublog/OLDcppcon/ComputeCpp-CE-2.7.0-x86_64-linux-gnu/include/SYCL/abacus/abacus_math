///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/modf.html
abacus_double8 ABACUS_EXPORT_API __abacus_modf(abacus_double8 x, __local abacus_double8 * whole_number);

/// @brief Wrapper for __abacus_modf.
/// @param[in]  x A abacus_double8.
/// @param[out] whole_number A abacus_double8.
/// @return     A abacus_double8.

inline abacus_double8 ABACUS_API modf(abacus_double8 x, __local abacus_double8 * whole_number) {
  return __abacus_modf(x, whole_number);
}
#endif

