/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number The floor @a x.
/// @return                  A abacus_float2.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float2 ABACUS_EXPORT_API __abacus_fract(abacus_float2 x, abacus_float2 * whole_number);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number A abacus_float2.
/// @return                  A abacus_float2.

inline abacus_float2 ABACUS_API fract(abacus_float2 x, abacus_float2 * whole_number) {
  return __abacus_fract(x, whole_number);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number A abacus_float2.
/// @return                  A abacus_float2.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float2 ABACUS_EXPORT_API __abacus_fract(abacus_float2 x, __global abacus_float2 * whole_number);

/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number A abacus_float2.
/// @return                  A abacus_float2.

inline abacus_float2 ABACUS_API fract(abacus_float2 x, __global abacus_float2 * whole_number) {
  return __abacus_fract(x, whole_number);
}

/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number A abacus_float2.
/// @return                  A abacus_float2.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float2 ABACUS_EXPORT_API __abacus_fract(abacus_float2 x, __local abacus_float2 * whole_number);

/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float2.
/// @param[out] whole_number A abacus_float2.
/// @return                  A abacus_float2.

inline abacus_float2 ABACUS_API fract(abacus_float2 x, __local abacus_float2 * whole_number) {
  return __abacus_fract(x, whole_number);
}
#endif

