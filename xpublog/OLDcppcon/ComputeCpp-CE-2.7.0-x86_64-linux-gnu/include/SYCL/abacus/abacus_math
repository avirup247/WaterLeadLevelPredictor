/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/sincos.html
abacus_double ABACUS_EXPORT_API __abacus_sincos(abacus_double x, __global abacus_double * whole_number);

/// @brief Wrapper for __abacus_sincos.
/// @param[in]  x A abacus_double.
/// @param[out] whole_number A abacus_double.
/// @return     A abacus_double.

inline abacus_double ABACUS_API sincos(abacus_double x, __global abacus_double * whole_number) {
  return __abacus_sincos(x, whole_number);
}

