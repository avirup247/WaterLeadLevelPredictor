#endif

/// @brief x^(1/y).
/// @param[in] x A abacus_float8.
/// @param[in] y A abacus_int8.
/// @return      A abacus_float8.
///
/// Standards compliant implementation of OpenCL 1.2 rootn.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/rootn.html
abacus_float8 ABACUS_EXPORT_API __abacus_rootn(abacus_float8 x, abacus_int8 y);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_rootn.
/// @param[in] x A abacus_float8.
/// @param[in] y A abacus_int8.
/// @return      A abacus_float8.
inline abacus_float8 ABACUS_API rootn(abacus_float8 x, abacus_int8 y) {
  return __abacus_rootn(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

