#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_atan2pi.
/// @param[in] y A abacus_float2.
/// @param[in] x A abacus_float2.
/// @return      A abacus_float2.
inline abacus_float2 ABACUS_API atan2pi(abacus_float2 y, abacus_float2 x) {
  return __abacus_atan2pi(y, x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

