/// @brief Log gamma with sign function.
/// @param[in] x         The number to process.
/// @param[out] out_sign The sign of the gamma function.
/// @return              A abacus_float16.
///
/// Standards compliant implementation of OpenCL 1.2 lgamma_r.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/lgamma.html
abacus_float16 ABACUS_EXPORT_API __abacus_lgamma_r(abacus_float16 x, abacus_int16 * out_sign);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_lgamma_r.
/// @param[in]  x A abacus_float16.
/// @param[out] out_sign The sign of the gamma function.
/// @return     A abacus_float16.

inline abacus_float16 ABACUS_API lgamma_r(abacus_float16 x, abacus_int16 * out_sign) {
  return __abacus_lgamma_r(x, out_sign);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Log gamma with sign function.
/// @param[in]  x        A abacus_float16.
/// @param[out] out_sign The sign of the gamma function.
/// @return              A abacus_float16.
///
/// Standards compliant implementation of OpenCL 1.2 lgamma_r.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/lgamma.html
abacus_float16 ABACUS_EXPORT_API __abacus_lgamma_r(abacus_float16 x, __global abacus_int16 * out_sign);

/// @brief Wrapper for __abacus_lgamma_r.
/// @param[in]  x A abacus_float16.
/// @param[out] out_sign The sign of the gamma function.
/// @return     A abacus_float16.

inline abacus_float16 ABACUS_API lgamma_r(abacus_float16 x, __global abacus_int16 * out_sign) {
  return __abacus_lgamma_r(x, out_sign);
}

/// @brief Log gamma with sign function.
/// @param[in]  x        A abacus_float16.
/// @param[out] out_sign The sign of the gamma function.
/// @return              A abacus_float16.
///
/// Standards compliant implementation of OpenCL 1.2 lgamma_r.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/lgamma.html
abacus_float16 ABACUS_EXPORT_API __abacus_lgamma_r(abacus_float16 x, __local abacus_int16 * out_sign);

/// @brief Wrapper for __abacus_lgamma_r.
/// @param[in]  x A abacus_float16.
/// @param[out] out_sign The sign of the gamma function.
/// @return     A abacus_float16.

inline abacus_float16 ABACUS_API lgamma_r(abacus_float16 x, __local abacus_int16 * out_sign) {
  return __abacus_lgamma_r(x, out_sign);
}
#endif

