#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_acospi.
/// @param[in] x A abacus_float.
/// @return      A abacus_float.
inline abacus_float ABACUS_API acospi(abacus_float x) {
  return __abacus_acospi(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

