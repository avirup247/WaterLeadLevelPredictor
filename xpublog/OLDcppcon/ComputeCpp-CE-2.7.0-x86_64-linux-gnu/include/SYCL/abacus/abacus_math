#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_ilogb.
/// @param[in] x A abacus_float4.
/// @return      A abacus_int4.
inline abacus_int4 ABACUS_API ilogb(abacus_float4 x) {
  return __abacus_ilogb(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

