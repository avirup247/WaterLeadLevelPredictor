#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_nextafter.
/// @param[in] x A abacus_double16.
/// @param[in] y A abacus_double16.
/// @return      A abacus_double16.
inline abacus_double16 ABACUS_API nextafter(abacus_double16 x, abacus_double16 y) {
  return __abacus_nextafter(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

