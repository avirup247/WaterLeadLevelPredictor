/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number The floor @a x.
/// @return                  A abacus_float3.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float3 ABACUS_EXPORT_API __abacus_fract(abacus_float3 x, abacus_float3 * whole_number);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number A abacus_float3.
/// @return                  A abacus_float3.

inline abacus_float3 ABACUS_API fract(abacus_float3 x, abacus_float3 * whole_number) {
  return __abacus_fract(x, whole_number);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number A abacus_float3.
/// @return                  A abacus_float3.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float3 ABACUS_EXPORT_API __abacus_fract(abacus_float3 x, __global abacus_float3 * whole_number);

/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number A abacus_float3.
/// @return                  A abacus_float3.

inline abacus_float3 ABACUS_API fract(abacus_float3 x, __global abacus_float3 * whole_number) {
  return __abacus_fract(x, whole_number);
}

/// @brief Fractional part of x function.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number A abacus_float3.
/// @return                  A abacus_float3.
///
/// Standards compliant implementation of OpenCL 1.2 fract.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/fract.html
abacus_float3 ABACUS_EXPORT_API __abacus_fract(abacus_float3 x, __local abacus_float3 * whole_number);

/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float3.
/// @param[out] whole_number A abacus_float3.
/// @return                  A abacus_float3.

inline abacus_float3 ABACUS_API fract(abacus_float3 x, __local abacus_float3 * whole_number) {
  return __abacus_fract(x, whole_number);
}
#endif

/// @brief Extract mantissa and exponent x function.
/// @param[in]  x        A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return              A abacus_float3, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 frexp.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/frexp.html
abacus_float3 ABACUS_EXPORT_API __abacus_frexp(abacus_float3 x, abacus_int3 * exponent);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_frexp.
/// @param[in]  x A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return     A abacus_float3.

inline abacus_float3 ABACUS_API frexp(abacus_float3 x, abacus_int3 * exponent) {
  return __abacus_frexp(x, exponent);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Extract mantissa and exponent x function.
/// @param[in]  x        A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return              A abacus_float3, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 frexp.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/frexp.html
abacus_float3 ABACUS_EXPORT_API __abacus_frexp(abacus_float3 x, __global abacus_int3 * exponent);

/// @brief Wrapper for __abacus_frexp.
/// @param[in]  x A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return     A abacus_float3.

inline abacus_float3 ABACUS_API frexp(abacus_float3 x, __global abacus_int3 * exponent) {
  return __abacus_frexp(x, exponent);
}

/// @brief Extract mantissa and exponent x function.
/// @param[in]  x        A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return              A abacus_float3, in range [0.5f..1.0f).
