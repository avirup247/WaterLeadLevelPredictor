#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_fract.
/// @param[in]  x            A abacus_float8.
/// @param[out] whole_number A abacus_float8.
/// @return                  A abacus_float8.

inline abacus_float8 ABACUS_API fract(abacus_float8 x, abacus_float8 * whole_number) {
  return __abacus_fract(x, whole_number);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
