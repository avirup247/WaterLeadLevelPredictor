#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_acosh.
/// @param[in] x A abacus_double16.
/// @return      A abacus_double16.
inline abacus_double16 ABACUS_API acosh(abacus_double16 x) {
  return __abacus_acosh(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

