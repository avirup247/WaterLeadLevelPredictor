/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/tan.html
abacus_double3 ABACUS_EXPORT_API __abacus_tanh(abacus_double3 x);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_tanh.
/// @param[in] x A abacus_double3.
/// @return      A abacus_double3.
inline abacus_double3 ABACUS_API tanh(abacus_double3 x) {
  return __abacus_tanh(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

