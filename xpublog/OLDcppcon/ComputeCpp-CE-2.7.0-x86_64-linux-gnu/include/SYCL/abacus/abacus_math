#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_atan2pi.
/// @param[in] y A abacus_float4.
/// @param[in] x A abacus_float4.
/// @return      A abacus_float4.
inline abacus_float4 ABACUS_API atan2pi(abacus_float4 y, abacus_float4 x) {
  return __abacus_atan2pi(y, x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

