/// @brief x^y, x >= 0.
/// @param[in] x A abacus_float2.
/// @param[in] y A abacus_float2.
/// @return      A abacus_float2.
///
/// Standards compliant implementation of OpenCL 1.2 powr.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/pow.html
abacus_float2 ABACUS_EXPORT_API __abacus_powr(abacus_float2 x, abacus_float2 y);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_powr.
/// @param[in] x A abacus_float2.
/// @param[in] y A abacus_float2.
/// @return      A abacus_float2.
inline abacus_float2 ABACUS_API powr(abacus_float2 x, abacus_float2 y) {
  return __abacus_powr(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

/// @brief @a x - (round(@a x / @a y) * @a y).
/// @param[in] x A abacus_float2.
/// @param[in] y A abacus_float2.
/// @return      A abacus_float2.
///
/// Standards compliant implementation of OpenCL 1.2 remainder.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remainder.html
abacus_float2 ABACUS_EXPORT_API __abacus_remainder(abacus_float2 x, abacus_float2 y);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remainder.
/// @param[in] x A abacus_float2.
/// @param[in] y A abacus_float2.
/// @return      A abacus_float2.
inline abacus_float2 ABACUS_API remainder(abacus_float2 x, abacus_float2 y) {
  return __abacus_remainder(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float2.
/// @param[in]  y        A abacus_float2.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float2, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float2 ABACUS_EXPORT_API __abacus_remquo(abacus_float2 x, abacus_float2 y, abacus_int2 * quotient);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float2.
/// @param[in]  y        A abacus_float2.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float2.

inline abacus_float2 ABACUS_API remquo(abacus_float2 x, abacus_float2 y, abacus_int2 * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float2.
/// @param[in]  y        A abacus_float2.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float2, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float2 ABACUS_EXPORT_API __abacus_remquo(abacus_float2 x, abacus_float2 y, __global abacus_int2 * quotient);

/// @brief Wrapper for __abacus_remquo.
