///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/log1p.html
abacus_double2 ABACUS_EXPORT_API __abacus_log1p(abacus_double2 x);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_log1p.
/// @param[in] x A abacus_double2.
/// @return      A abacus_double2.
inline abacus_double2 ABACUS_API log1p(abacus_double2 x) {
  return __abacus_log1p(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

