#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_rsqrt.
/// @param[in] x A abacus_float16.
/// @return      A abacus_float16.
inline abacus_float16 ABACUS_API rsqrt(abacus_float16 x) {
  return __abacus_rsqrt(x);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

