/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/frexp.html
abacus_float3 ABACUS_EXPORT_API __abacus_frexp(abacus_float3 x, __local abacus_int3 * exponent);

/// @brief Wrapper for __abacus_frexp.
/// @param[in]  x A abacus_float3.
/// @param[out] exponent The exponent of x.
/// @return     A abacus_float3.

inline abacus_float3 ABACUS_API frexp(abacus_float3 x, __local abacus_int3 * exponent) {
  return __abacus_frexp(x, exponent);
}
#endif

