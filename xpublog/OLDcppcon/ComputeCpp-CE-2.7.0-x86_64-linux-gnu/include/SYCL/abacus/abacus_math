#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_copysign.
/// @param[in] x A abacus_double.
/// @param[in] y A abacus_double.
/// @return      A abacus_double.
inline abacus_double ABACUS_API copysign(abacus_double x, abacus_double y) {
  return __abacus_copysign(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

