  return __abacus_powr(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

/// @brief @a x - (round(@a x / @a y) * @a y).
/// @param[in] x A abacus_float.
/// @param[in] y A abacus_float.
/// @return      A abacus_float.
///
/// Standards compliant implementation of OpenCL 1.2 remainder.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remainder.html
abacus_float ABACUS_EXPORT_API __abacus_remainder(abacus_float x, abacus_float y);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remainder.
/// @param[in] x A abacus_float.
/// @param[in] y A abacus_float.
/// @return      A abacus_float.
inline abacus_float ABACUS_API remainder(abacus_float x, abacus_float y) {
  return __abacus_remainder(x, y);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float ABACUS_EXPORT_API __abacus_remquo(abacus_float x, abacus_float y, abacus_int * quotient);

#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
namespace abacus {
#endif
/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float.

inline abacus_float ABACUS_API remquo(abacus_float x, abacus_float y, abacus_int * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#if defined(__cplusplus) && !defined(__OPENCL_VERSION__)
}
#endif

#if defined(__OPENCL_VERSION__) && !defined(__cplusplus)
/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float ABACUS_EXPORT_API __abacus_remquo(abacus_float x, abacus_float y, __global abacus_int * quotient);

/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float.

inline abacus_float ABACUS_API remquo(abacus_float x, abacus_float y, __global abacus_int * quotient) {
  return __abacus_remquo(x, y, quotient);
}

/// @brief Remainder and quotient function.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return              A abacus_float, in range [0.5f..1.0f).
///
/// Standards compliant implementation of OpenCL 1.2 remquo.
///
/// @see https://www.khronos.org/registry/cl/sdk/1.2/docs/man/xhtml/remquo.html
abacus_float ABACUS_EXPORT_API __abacus_remquo(abacus_float x, abacus_float y, __local abacus_int * quotient);

/// @brief Wrapper for __abacus_remquo.
/// @param[in]  x        A abacus_float.
/// @param[in]  y        A abacus_float.
/// @param[out] quotient The quotient of x.
/// @return     A abacus_float.

inline abacus_float ABACUS_API remquo(abacus_float x, abacus_float y, __local abacus_int * quotient) {
  return __abacus_remquo(x, y, quotient);
}
#endif

